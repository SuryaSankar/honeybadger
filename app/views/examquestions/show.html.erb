<%- model_class = Examquestion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:question_id) %>:</strong></dt>
  <dd><%= @examquestion.question_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qpaper_id) %>:</strong></dt>
  <dd><%= @examquestion.qpaper_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mark) %>:</strong></dt>
  <dd><%= @examquestion.mark %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qnumber) %>:</strong></dt>
  <dd><%= @examquestion.qnumber %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              examquestions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_examquestion_path(@examquestion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              examquestion_path(@examquestion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
