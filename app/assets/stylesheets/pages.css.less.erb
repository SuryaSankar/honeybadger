.whiteOnGreen{
	background-color: @boardGreen;
	color: @white;
	padding-top: 10px;
	margin-bottom: 0px;
}
@font-face {
  font-family: 'crayon';    
  src: url(<%= asset_path 'crayon.eot' %>);
  src: url('/assets/crayon.eot?iefix') format('embedded-opentype'),
  url(<%= asset_path 'crayon.woff' %>) format('woff'),
  url(<%= asset_path 'crayon.ttf' %>) format('truetype'),
  url('/assets/crayon.svg#crayon') format('svg');
  font-weight: normal;
  font-style: normal;
}
@fbBlue: #0033FF;
.blueText{
	color: @fbBlue;
}

.redText{
	color: red;
}

.navbarCursiveFont{
	font-family: 'Special Elite' , cursive;
	font-weight: bold;
	font-size: 15px;
	  li { font-size: 15px;
		font-weight: bold;
		font-family: 'Special Elite', cursive;
	    a { text-decoration: none; 
		font-family: 'Special Elite', cursive;
	      &:hover { border-width: 1px; color: @black; }
	    }
	  }
}

.handwriting(@family: 'Special Elite', @fontColor: @white){
	font-family: @family;
	color: @fontColor;
	font-size: 15px;
	    a { text-decoration: underline; 
		color: @fontColor;
		font-family: 'Special Elite', cursive;
		font-weight: bold;
		font-size: 15px;
		cursor:pointer;
	      &:hover { border-width: 1px; color: @fontColor; }
	    }
	  h1 {
	    font-size: 50px;
	  }
	p{
	font-size: 15px;
	font-family: @family;
	    a { text-decoration: underline; 
		color: @fontColor;
		font-family: 'Special Elite', cursive;
		font-weight: bold;
		font-size: 15px;
		cursor:pointer;
	      &:hover { border-width: 1px; color: @fontColor; }
	    }
	}	
	  li { font-size: 15px;
		color: @fontColor;
		font-family: 'Special Elite', cursive;
	    a { text-decoration: none; 
		color: @fontColor;
		font-family: 'Special Elite', cursive;
	      &:hover { border-width: 1px; color: @fontColor; }
	    }
	  }
}

.handwriting{
	.handwriting;
}

.navBarHandwriting{
	.handwriting(@family: 'Special Elite',@fontColor: @boardGreen);
}

#banner{
	color: @white;
	.border-radius(0px);
	margin-bottom: 0px;
	padding-top: 20px;
	background-color: @boardGreen;
	height: 130px;
	.handwriting;
}
.overlay{
    position:fixed;
    width:100%;
    height:100%;
    top:0px;
    left:0px;
    z-index:1000;
}

#homeBG{
	padding: 60px;
	margin-left: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	font-size: 18px;
	font-weight: 200;
	line-height: @baseLineHeight * 2;
	.border-radius(6px);
	color: @white;
	.border-radius(0px);
	margin-bottom: 0px;
	padding-top: 20px;
	background-color: @boardGreen;
	.handwriting;
	.overlay;
	h1 {
	margin-bottom: 0;
	font-size: 80px;
	line-height: 1.5;
	color: @white;
	letter-spacing: -1px;
	}
	li {
	line-height: @baseLineHeight * 1.5; // Reset since we specify in type.less
	}
}

#signup_modal .modal-body {
	max-height: 800px;
}
.topPadding20{
	padding-top: 20px;
}
#lowerBar{
	height: 25px;
	background-color: @woodBrown;
}

.woodTexture{
	background-image:url('/assets/wood.jpg');
}

#tree_box{
	padding: 5px;
	border: 2px solid;
	border-radius: 10px;
	height: 350px;
}

#login_box{
	padding: 5px;
	border: 2px solid;
	border-radius: 10px;
}
